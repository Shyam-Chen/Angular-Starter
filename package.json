{
  "name": "angular-starter",
  "version": "1.0.0",
  "description": "A boilerplate for HTML5, Angular, Material, TypeScript, and ReactiveX.",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "start": "rimraf public && webpack-dev-server --progress",
    "build": "rimraf public && webpack --env.prod",
    "lint": "tslint 'src/**/*.ts' && stylelint 'src/**/*.css'",
    "unit": "cross-env JEST_ENV=app jest \"src/.*\\.ts$\" --coverage --forceExit --detectOpenHandles",
    "e2e": "cross-env JEST_ENV=e2e jest \"src/.*\\.ts$\" --forceExit --detectOpenHandles",
    "codecov": "codecov",
    "firebase": "firebase"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Angular-Starter.git"
  },
  "author": "Shyam Chen",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "9.0.0",
    "@angular/cdk": "9.0.0",
    "@angular/common": "9.0.0",
    "@angular/core": "9.0.0",
    "@angular/flex-layout": "9.0.0-beta.29",
    "@angular/forms": "9.0.0",
    "@angular/material": "9.0.0",
    "@angular/platform-browser": "9.0.0",
    "@angular/platform-browser-dynamic": "9.0.0",
    "@angular/router": "9.0.0",
    "@ngxs/devtools-plugin": "3.2.0",
    "@ngxs/logger-plugin": "3.2.0",
    "@ngxs/router-plugin": "3.2.0",
    "@ngxs/store": "3.2.0",
    "core-js": "3.6.4",
    "hammerjs": "2.0.8",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular/compiler": "9.0.0",
    "@angular/compiler-cli": "9.0.0",
    "@angular/language-service": "9.0.0",
    "@ngtools/webpack": "9.0.1",
    "@types/jest": "23.3.2",
    "@types/node": "10.17.14",
    "codecov": "3.6.5",
    "codelyzer": "4.4.4",
    "copy-webpack-plugin": "4.6.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.1",
    "cssnano": "4.1.7",
    "file-loader": "2.0.0",
    "firebase-tools": "6.6.0",
    "html-webpack-plugin": "4.0.0-alpha",
    "jest": "23.6.0",
    "jest-preset-angular": "8.0.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.1.0",
    "postcss-preset-env": "6.4.0",
    "prettier": "^1.18.2",
    "process-envify": "0.1.3",
    "puppeteer": "1.10.0",
    "raw-loader": "0.5.1",
    "rucksack-css": "1.0.2",
    "run-sequence": "2.2.1",
    "script-ext-html-webpack-plugin": "2.1.3",
    "style-loader": "0.23.1",
    "stylelint": "9.9.0",
    "stylelint-config-standard": "18.2.0",
    "to-string-loader": "1.1.5",
    "ts-jest": "23.10.5",
    "ts-node": "7.0.1",
    "tslint": "5.17.0",
    "tslint-angular": "3.0.2",
    "tslint-config-airbnb": "5.11.1",
